{
  "name": "minsky",
  "version": "7.2.1",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main && exitzero electron-builder install-app-deps",
    "nx": "nx",
    "start": "npm-run-all -p web electron",
    "start:log": "DEBUG=minsky:* DEBUG_COLOR=true npm run start",
    "web": "nx serve",
    "electron": "wait-on http-get://localhost:4200/ && nx serve minsky-electron",
    "build:web": "nx build minsky-web",
    "build:electron": "nx build minsky-electron",
    "export:package:windows": "npm run build:web && npm run build:electron && nx run minsky-electron:make --platform windows",
    "export:package:linux": "npm run build:web && npm run build:electron && nx run minsky-electron:make --platform linux --arch x64 --publishPolicy never",
    "export:package:mac": "npm run build:web && npm run build:electron && nx run minsky-electron:make --platform mac",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "workspace-generator": "nx workspace-generator",
    "bump:major": "nx run workspace:version --version=major",
    "bump:minor": "nx run workspace:version --version=minor",
    "bump:patch": "nx run workspace:version --version=patch",
    "bump:alpha": "nx run workspace:version --version=prerelease --preid=alpha",
    "bump:beta": "nx run workspace:version --version=prerelease --preid=beta"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.2.8",
    "@angular/cdk": "11.2.7",
    "@angular/common": "11.2.8",
    "@angular/compiler": "11.2.8",
    "@angular/core": "11.2.8",
    "@angular/forms": "11.2.8",
    "@angular/material": "11.2.7",
    "@angular/platform-browser": "11.2.8",
    "@angular/platform-browser-dynamic": "11.2.8",
    "@angular/router": "11.2.8",
    "@nrwl/angular": "12.0.4",
    "@nrwl/linter": "12.0.4",
    "@nrwl/tao": "12.0.4",
    "JSONStream": "^1.3.5",
    "angular-resize-event": "^2.0.1",
    "debug": "^4.3.1",
    "electron-context-menu": "^2.5.0",
    "electron-log": "^4.3.3",
    "electron-store": "^7.0.3",
    "express": "4.17.1",
    "is-electron": "^2.2.0",
    "keysym": "0.0.6",
    "mousetrap": "^1.6.5",
    "ngx-auto-unsubscribe": "^3.0.1",
    "ngx-socket-io": "^3.3.1",
    "p-queue": "^6.6.2",
    "rxjs": "^6.6.7",
    "socket.io": "^2.4.1",
    "tslib": "^2.2.0",
    "utf8": "^3.0.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.8",
    "@angular-eslint/eslint-plugin": "2.0.2",
    "@angular-eslint/eslint-plugin-template": "2.0.2",
    "@angular-eslint/template-parser": "2.0.2",
    "@angular/cli": "11.2.7",
    "@angular/compiler-cli": "11.2.8",
    "@angular/language-service": "11.2.8",
    "@compodoc/compodoc": "^1.1.11",
    "@jscutlery/semver": "^2.1.0",
    "@ngx-translate/core": "12.1.2",
    "@ngx-translate/http-loader": "5.0.0",
    "@nrwl/cli": "12.0.4",
    "@nrwl/cypress": "12.0.4",
    "@nrwl/eslint-plugin-nx": "12.0.4",
    "@nrwl/express": "12.0.4",
    "@nrwl/jest": "12.0.4",
    "@nrwl/node": "12.0.4",
    "@nrwl/workspace": "12.0.4",
    "@twittwer/compodoc": "1.5.1",
    "@types/express": "4.17.0",
    "@types/jest": "26.0.8",
    "@types/keysym": "0.0.1",
    "@types/mousetrap": "^1.6.6",
    "@types/node": "14.14.33",
    "@types/socket.io": "^2.1.11",
    "@types/utf8": "^2.1.6",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "cypress": "^6.0.1",
    "dotenv": "8.2.0",
    "electron": "^11.4.2",
    "eslint": "7.18.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "exitzero": "^1.0.1",
    "husky": "^4.3.8",
    "jest": "26.2.2",
    "jest-preset-angular": "8.3.2",
    "nx-electron": "11.0.2",
    "prettier": "2.2.1",
    "ts-jest": "26.4.0",
    "ts-node": "9.1.1",
    "typescript": "~4.0.3",
    "wait-on": "^5.3.0"
  },
  "engines": {
    "node": "14",
    "vscode": "^1.50.0",
    "npm": "~7.5.4"
  }
}
