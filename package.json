{
  "name": "minsky",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points && npm run postinstall:electron && exitzero electron-builder install-app-deps",
    "nx": "nx",
    "postinstall:web": "node tools/builders/postinstall-web",
    "postinstall:electron": "node tools/builders/postinstall",
    "start": "npm run postinstall:electron && npm-run-all -p web electron",
    "start:log": "DEBUG=minsky:* DEBUG_COLOR=true npm run start",
    "web": "npm run postinstall:web && nx serve",
    "electron": "wait-on http-get://localhost:4200/ && nx serve minsky-electron",
    "build:web": "nx build minsky-web",
    "build:electron": "nx build minsky-electron",
    "export:package:windows": "npm run build:web && npm run build:electron && nx run minsky-electron:make --platform windows",
    "export:package:linux": "npm run build:web && npm run build:electron && nx run minsky-electron:make --platform linux",
    "export:package:mac": "npm run build:web && npm run build:electron && nx run minsky-electron:make --platform mac",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help"
  },
  "private": true,
  "dependencies": {
    "@angular-material-components/color-picker": "^4.0.4",
    "@angular/animations": "^10.2.4",
    "@angular/cdk": "~10.2.7",
    "@angular/common": "^10.2.4",
    "@angular/compiler": "^10.2.4",
    "@angular/core": "^10.2.4",
    "@angular/forms": "^10.2.4",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "^10.2.4",
    "@angular/platform-browser-dynamic": "^10.2.4",
    "@angular/router": "^10.2.4",
    "@nrwl/angular": "^10.4.10",
    "angular-resize-event": "^2.0.1",
    "debug": "^4.3.1",
    "electron-json-storage": "^4.3.0",
    "electron-log": "^4.3.1",
    "electron-store": "^6.0.1",
    "express": "4.17.1",
    "get-port": "^5.1.1",
    "global": "^4.4.0",
    "is-electron": "^2.2.0",
    "ng-terminal": "^3.0.2",
    "ngx-socket-io": "^3.2.0",
    "portscanner": "^2.2.0",
    "rxjs": "~6.6.3",
    "socket.io": "^2.4.1",
    "socketio-file-upload": "^0.7.3",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.1",
    "@angular/cli": "^10.2.1",
    "@angular/compiler-cli": "^10.2.4",
    "@angular/language-service": "^10.2.4",
    "@babel/core": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-react": "7.9.4",
    "@babel/preset-typescript": "7.9.0",
    "@ngx-translate/core": "12.1.2",
    "@ngx-translate/http-loader": "5.0.0",
    "@nrwl/cli": "^10.4.10",
    "@nrwl/cypress": "^10.4.10",
    "@nrwl/eslint-plugin-nx": "^10.4.10",
    "@nrwl/express": "^10.4.10",
    "@nrwl/jest": "^10.4.10",
    "@nrwl/node": "^10.4.10",
    "@nrwl/workspace": "^10.4.10",
    "@types/electron-json-storage": "^4.0.0",
    "@types/electron-store": "^3.2.0",
    "@types/express": "4.17.0",
    "@types/jest": "26.0.8",
    "@types/node": "~8.9.4",
    "@types/socket.io": "^2.1.11",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "babel-jest": "26.2.2",
    "codelyzer": "~5.0.1",
    "conventional-changelog-cli": "2.0.34",
    "cypress": "^4.1.0",
    "dotenv": "6.2.0",
    "electron": "^11.2.0",
    "eslint": "7.18.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-cypress": "^2.10.3",
    "exitzero": "^1.0.1",
    "husky": "^4.3.8",
    "jest": "26.2.2",
    "jest-preset-angular": "8.3.1",
    "nx-electron": "^10.0.3",
    "prettier": "2.2.1",
    "spectron": "^11.1.0",
    "ts-jest": "26.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.0.0",
    "typescript": "~4.0.3",
    "wait-on": "^5.2.1"
  },
  "engines": {
    "node": "14",
    "vscode": "^1.50.0",
    "npm": "~7.5.4"
  }
}
